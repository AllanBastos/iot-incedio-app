{
  "expo": {
    "name": "incedio-app",
    "slug": "incedio-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.iot.detector_de_incedio",
      "versionCode": 1,
      "permissions": [
        "NOTIFICATIONS",
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/notification_sound.mp3",
            "./assets/notification_sound_other.mp3"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c8a0618d-30ba-4318-b0a5-d265901d083f"
      }
    }
  }
}
